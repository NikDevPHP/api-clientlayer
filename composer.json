{
  "name": "andersonef/api-clientlayer",
  "description": "Laravel Api Implementation -  Client layer",
  "keywords": [
    "laravel",
    "api",
    "client",
    "curl"
  ],
  "licence": "MIT",
  "authors": [
    {
      "name": "Anderson Nunes",
      "email": "anderson.nuneseth@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "illuminate/database":"5.1.*",
    "illuminate/auth":"5.1.*",
    "illuminate/support":"5.1.*",
    "illuminate/console": "5.0.x|5.1.x",
    "illuminate/filesystem": "5.0.x|5.1.x",
    "symfony/class-loader": "~2.3",
    "firebase/php-jwt": "^2.2.0",
    "anlutro/curl": "^1.3.1"
  },
  "autoload": {
    "psr-4": {
      "Andersonef\\ApiClientLayer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Andersonef\\Tests\\ApiClientLayer\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
